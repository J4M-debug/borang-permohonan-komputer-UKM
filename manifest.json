{
    "name": "Borang Permohonan Komputer UKM",
    "short_name": "Pinjaman Komputer",
    "description": "Aplikasi borang permohonan pinjaman komputer UKM.",
    "start_url": "/index.html",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#007bff",
    "icons": [
      {
        "src": "icons/icon-192x192.png",
        "sizes": "192x192",
        "type": "image/png"
      },
      {
        "src": "icons/icon-512x512.png",
        "sizes": "512x512",
        "type": "image/png"
      }
    ],
    "permissions": ["storage", "notifications"],
    "serviceworker": {
      "src": "service-worker.js",
      "scope": "/",
      "update_via_cache": "none"
    },
    "notifications": {
      "enabled": true,
      "actions": [
        {
          "action": "view",
          "title": "Lihat Permohonan"
        },
        {
          "action": "dismiss",
          "title": "Tutup"
        }
      ]
    }
  }
  